<!-- 下面按钮 -->
<div class="w_button_box">
    <input type="button" id="save" value="确认" class="c_button"/>
    <input type="button" id="close" value="取消" class="button"/>
</div>
<script type="text/javascript">
    $(function(){
        resizeForm();
        $(".icon_lists li").click(function(){
            if($(this).hasClass("selected")){
                $(".selected").removeClass("selected");
            }
            $(".selected").removeClass("selected");
            $(this).addClass("selected");
        })
        $("#save").click(function(){
            if($(".selected").length == 0){
               layer.alert("未选择图标");
                return;
            }
            var func = GetQueryString("func");
            var evalFunc = eval("getPrevWin()."+func);
            evalFunc($(".selected").find(".code").text());
            closeWin();
        })
    })
</script>
