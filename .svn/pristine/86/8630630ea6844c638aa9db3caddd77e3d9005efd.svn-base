package net.ruixin.dictionary;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

/**
 * 字典Mapping
 */

@Controller
@RequestMapping("/dict")
public class DictMapping {

    @RequestMapping("/dictList")
    public String dictList() {
        return "baseModel/dictionary/dictList";
    }

    @RequestMapping("/dictEdit")
    public String dictEdit() {
        return "baseModel/dictionary/dictEdit";
    }

    @RequestMapping("/subDictEdit")
    public String subDictEdit() {
        return "baseModel/dictionary/subDictEdit";
    }

    @RequestMapping("/dictSelect")
    public String dictSelect() {
        return "baseModel/dictionary/dictSelect";
    }

}
